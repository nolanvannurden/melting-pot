               <form onSubmit={e => this.register(e)}>\n                    <input \n                        name=\"email\" \n                        value={email} \n                        placeholder=\"Email\" \n                        onChange={ e => this.changeHandler(e)}\n                    />\n                    <input \n                        name=\"username\" \n                        value={username} \n                        placeholder=\"username\" \n                        onChange={ e => this.changeHandler(e)}\n                    />\n                    <input \n                        name=\"password\" \n                        type=\"password\"\n                        value={password} \n                        placeholder=\"password\" \n                        onChange={ e => this.changeHandler(e)}\n                    />\n                    <button>Submit</button>\n                    <button onClick={this.toggleNewUser}>Already a user?</button>\n                     </form>\n\t\t\t\t</div>\n                </div>\t\t\n            </div>\n            :\n            <div>\n\t\t\t\t<div className=\"login-background\">\n                <div className=\"login-styling\">\n                 <h3>Login</h3>\n                    <form  onSubmit={e => this.login(e)}>\n                        <input \n                            name=\"email\" \n                            value={email} \n                            placeholder=\"Email\" \n                            onChange={ e => this.changeHandler(e)}\n                        />\n                        <input \n                            name=\"password\" \n                            type=\"password\"\n                            value={password} \n                            placeholder=\"password\" \n                            onChange={ e => this.changeHandler(e)}\n                        />\n                    <button>Submit</button>\n                    \n                        <button onClick={this.toggleNewUser}>Want to join?</button>\n                </form>\n                </div>\n                            <h4 className=\"letter-to-audience\">This is a website created for the sharing of recipes with your closest friends and family. The whole idea is to help each other perfect their cooking skills/recipes. This site was intended to help you discover new recipes and learn new cooking techniques.\n                            </h4>\n\t\t\t\t    </div>  \n                </div>}\n            </div>\n        )\n    }\n}\n\n\nconst mapDispatchToProps = {\n    loginUser\n};\n\nconst mapStateToProps = (reduxState) => {\n    const {user, isLoggedIn} = reduxState;\n    return{\n        user,\n        isLoggedIn\n    };\n};\n\nexport default connect(mapStateToProps,mapDispatchToProps)(withRouter(Auth))","/Users/nolanvannurden/melting-pot/melting-pot/src/redux/store.js",[],"/Users/nolanvannurden/melting-pot/melting-pot/src/components/Header/Header.js",[],"/Users/nolanvannurden/melting-pot/melting-pot/src/components/PostDisplay/Edit.js",[],"/Users/nolanvannurden/melting-pot/melting-pot/src/components/PostDisplay/PostDisplay.js",[],"/Users/nolanvannurden/melting-pot/melting-pot/src/routes.js",[],"/Users/nolanvannurden/melting-pot/melting-pot/src/components/PostDisplay/Post/Post.js",[],"/Users/nolanvannurden/melting-pot/melting-pot/src/components/Dashboard/Dashboard.js",[],"/Users/nolanvannurden/melting-pot/melting-pot/src/components/Recipes.js",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"66","replacedBy":"70"},{"ruleId":"68","replacedBy":"71"},{"ruleId":"72","severity":1,"message":"73","line":3,"column":9,"nodeType":"74","messageId":"75","endLine":3,"endColumn":13},"no-native-reassign",["76"],"no-negated-in-lhs",["77"],["76"],["77"],"no-unused-vars","'Link' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]